<launch>
  <arg name="use_rviz" default="false" description="Launch rviz"/>

  <include file="$(find-pkg-share crowd_nav)/launch/crowdnav.launch.xml">
      <arg name="sim_ped" value="false"/>
      <arg name="sim_odom" value="false"/>
      <arg name="launch_zed" value="true"/>
      <arg name="onboard" value="true" />
      <arg name="zed_localize" value="true" />
      <arg name="use_rviz" value="$(var use_rviz)" />
    </include>

</launch>
