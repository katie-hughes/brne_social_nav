<launch>

  <arg name="sim_odom" default="true" description="Simulate cmd_vel->odom update"/>

  <include file="$(find-pkg-share crowd_nav)/launch/crowdnav.launch.xml">
      <arg name="sim_ped" value="false"/>
      <arg name="sim_odom" value="$(var sim_odom)"/>
      <arg name="launch_zed" value="true"/>
      <arg name="onboard" value="false" />
      <arg name="zed_localize" value="false" />
    </include>

</launch>
